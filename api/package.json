{
  "name": "js-authentication",
  "version": "1.0.0",
  "description": "Node.js authentication",
  "main": "index.js",
  "scripts": {
    "client": "npm start --prefix client",
    "start": "nodemon  index.js -w",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "generate:tree": "mddir"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2imad/node-js-authentication.git"
  },
  "keywords": [
    "node.js"
  ],
  "author": "Bemoore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/2imad/node-js-authentication/issues"
  },
  "homepage": "https://github.com/2imad/node-js-authentication#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.13",
    "nodemailer": "^6.4.15",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "mddir": "^1.1.1",
    "nodemon": "^2.0.6"
  }
}
